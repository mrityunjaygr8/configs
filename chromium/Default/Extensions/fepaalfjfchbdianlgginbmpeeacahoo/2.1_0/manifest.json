{
   "author": "APKMirror",
   "background": {
      "persistent": false,
      "scripts": [ "js/jquery-3.2.1.min.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon19.png",
         "38": "img/icon38.png"
      },
      "default_popup": "popup-settings.html",
      "default_title": "Toolbox settings"
   },
   "content_scripts": [ {
      "css": [ "css/styles.css" ],
      "js": [ "js/jquery-3.2.1.min.js", "js/contentscript.js" ],
