{
   "background": {
      "scripts": [ "jquery.js", "downloader.js" ]
   },
   "content_scripts": [ {
      "js": [ "jquery.js", "grabContent.js" ],
      "matches": [ "http://*.pandora.com/*", "https://*.pandora.com/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Extend the functions of Pandora web listener for free!",
   "icons": {
      "128": "PandoraExtensionsLogo128.png",
      "16": "Icons/PandoraExtensionsLogo16.png",
      "48": "Icons/PandoraExtensionsLogo48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Pandora Extensions",
   "permissions": [ "webRequest", "webRequestBlocking", "http://*.pandora.com/", "https://*.pandora.com/", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.0",
   "web_accessible_resources": [ "WebFiles/downloadIcon.png", "WebFiles/youtubeIcon.png", "WebFiles/downloadButton.html", "WebFiles/youtubeButton.html" ]
}
